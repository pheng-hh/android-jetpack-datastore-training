<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.sfeir.school.android.datastore">

  <application
    android:name=".DataStoreApp"
    android:allowBackup="true"
    android:icon="@drawable/logo_sfeir_school"
    android:label="DataStore"
    android:roundIcon="@drawable/logo_sfeir_school_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.SFEIRSchoolDataStore">
    <activity
      android:name=".ui.activity.SportActivity"
      android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>
